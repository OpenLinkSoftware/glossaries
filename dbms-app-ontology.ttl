## Properties

:supportsIdentifierType
    a                owl:ObjectProperty ;
    rdfs:label       "supportsIdentifierType"@en ;
    rdfs:comment     """Associates a Database Management System (DBMS) with supported Identifier Types. 
                        Examples include IRIs, URIs, and others.
                     """@en ;
    rdfs:domain      :DatabaseManagementSystem ;
    rdfs:range       dbpedia-yago:Identifier107270601 ;
    skos:related     iri:this, uri:this ;
    rdfs:isDefinedBy : .

:identifierQuoteChar
    a                owl:ObjectProperty ;
    rdfs:label       "identifierQuoteChar"@en ;
    rdfs:comment     """Associates a Database Management System (DBMS) with supported Identifier 
                        Quotation character for system artifact naming/identification (relations/tables, 
                        inferred-relations/views, stored procedures), e.g., double-quote, single-quote, 
                        less-than, and greater-than characters.
                     """@en ;
    rdfs:domain      :DatabaseManagementSystem ;
    rdfs:range       xsd:literal ;
    skos:related     iri:this, uri:this ;
    rdfs:isDefinedBy : .

:relationsModeledAs
    a                owl:ObjectProperty ;
    rdfs:label       "relationsModeledAs"@en ;
    rdfs:comment     """Associates a Database Management System (DBMS) with how its Entity Relationship 
                        Types (Relations) are modeled, e.g., Tables, Graphs, Trees, Hybrid, etc.
                     """@en ;
    rdfs:domain      :DatabaseManagementSystem ;
    rdfs:range       dbpedia-yago:Sequence108459252 ;
    skos:related     tables:this , 
                     rdf-graphs:this , 
                     dbpedia:Directed_graph ;
    rdfs:isDefinedBy : .

:relationStructure
    a                owl:ObjectProperty ;
    rdfs:label       "relationStructure"@en ;
    rdfs:comment     """Associates a Database Management System (DBMS) with how its Entity Relationship 
                        Types (Relations) are structured, e.g., N-Tuples, 3-Tuples, Key-Value Pairs, etc.
                     """@en ;
    rdfs:domain      :DatabaseManagementSystem ;
    rdfs:range       dbpedia-yago:DataStructure105728493 ;
    skos:related     dbpedia:Semantic_triple , 
                     dbpedia:Tuple , 
                     tuple:this ;
    rdfs:isDefinedBy : .

:relationPartitioning
    a                owl:ObjectProperty ;
    rdfs:label       "relationPartitioning"@en ;
    rdfs:comment     """Associates a Database Management System (DBMS) with how its relations are partitioned
                        for unambiguous identification using namespaces. For example: catalog.schema.relation,
                        database.schema.relation, schema.relation, urn:xyz:data, http://example.com/entity/,
                        http://example.com/entity#this, etc.
                     """@en ;
    rdfs:domain      :DatabaseManagementSystem ;
    rdfs:range       xsd:literal ;
    skos:related     dbpedia:Semantic_triple , 
                     dbpedia:Tuple , 
                     tuple:this ;
    rdfs:isDefinedBy : .

:tupleNotation
    a                owl:ObjectProperty ;
    rdfs:label       "tupleNotation"@en ;
    rdfs:comment     """Associates a Database Management System (DBMS) with the notations it supports for
                        representing N-Tuples, 3-Tuples, etc.
                     """@en ;
    rdfs:domain      :DatabaseManagementSystem ;
    rdfs:range       dbpedia-yago:Notation106808493 ;
    skos:related     rdf-turtle:this , 
                     rdf-ntriples:this , 
                     rdf-xml:this , 
                     json-ld:this , 
                     html-microdata:this ,
                     xhtml-rdfa:this , 
                     csv:this, 
                     tuple:this ;
    rdfs:isDefinedBy : .

:declarativeDataDefinitionLang
    a                owl:ObjectProperty ;
    rdfs:label       "declarativeDataDefinitionLang"@en ;
    skos:altLabel    "DDL" ;
    rdfs:comment     """Associates a Database Management System (DBMS) with the declarative
                        Data Definition Languages it supports.
                     """@en ;
    rdfs:domain      :DatabaseManagementSystem ;
    rdfs:range       dbpedia-yago:WikicatDataModelingLanguages ;
    skos:related     rdf-ddl:this , 
                     owl-ddl:this , 
                     sql-ddl:this ;
    rdfs:isDefinedBy : .

:declarativeDataManipulationLang
    a                owl:ObjectProperty ;
    rdfs:label       "declarativeDataManipulationLang"@en ;
    skos:altLabel    "DML" ;
